{
    "appId": "*****",
    "token": "*****",
    "channelName": "recorderTest",
    "useStringUid": false,
    "useCloudProxy": false,
    "userId": "0",
    "subAllAudio": true,         // 是否订阅所有音频，如果为false,在SubAudioUserList中填入订阅的uid
