    "maxResolutionUid": "123",   // 在vertical布局中，设定显示最大分辨率的uid
    "recorderStreamType":"both",   // 录制类型， audio_only 只录音频，video_only 只录音频， both 音视频都录
    "recorderPath":"recorderTest.mp4",  // 合流录制时为录制的文件名，单流录制为录制的目录，以每一个uid为名的mp4文件
